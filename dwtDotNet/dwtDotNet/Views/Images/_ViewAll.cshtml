@model IEnumerable<dwtDotNet.Share.Entites.image>

<flash dismissable="true" />



<table class="table table-striped table-responsive-md table-bordered smfont" id="UsersTable">
    <colgroup>
        <col span="1" />
        <col span="1" />
        <col span="1" />
        <col span="1" />
        <col span="1" />
        <col span="1" />
        <col span="1" />
        <col span="1" />
        <col span="1" />
        <col span="1" />
        <col span="1" />
    </colgroup>
    <thead>
        <tr>          
            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>
           
            <th>
                @Html.DisplayNameFor(model => model.ImageFullPath)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>               
                <td>
                    @Html.DisplayFor(modelItem => item.Title)
                </td>
            
                <td>
                    <div class="zoom">
                        <img src="@item.ImageFullPath" style="width:100px; height:100px; border-radius:50px" />
                    </div>
                </td>
            </tr>
        }
    </tbody>
</table>